<launch>  

  <node pkg="my_robot" type="serial_process.py" name="serial_process" output="screen" >
  <remap from="imu/da" to="imu/data"/>
  </node>
  
  <node pkg="tf" type="static_transform_publisher" name="laser_tf" args="0 0 0 0 0 0 /map /laser 100" />
  
  <!-- <node pkg="tf" type="static_transform_publisher" name="camera_tf" args="0 0 0 0 0 0 /base_link /camera_link 100" /> -->
  
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find my_robot)/rviz/teb.rviz" />

</launch>
